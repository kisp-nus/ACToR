{"name": "empty_input_file", "description": "Test with empty input file", "alias_name": "", "check_file": true, "cmd_prep": "mkdir -p test_files && touch test_files/empty", "cmd_target": ["cd test_files && ../BINARY empty 1 > /dev/null || true", "cd test_files && test -f xx00 && cat xx00 || echo 'No output file'"], "cmd_post": "rm -rf test_files", "idx": 1}
