{"name": "regex_with_special_chars", "description": "Test regex with special characters", "alias_name": "", "check_file": true, "cmd_prep": "mkdir -p test_files && printf \"line [1]\\nline (2)\\nline {3}\\nline .4.\" > test_files/special", "cmd_target": ["cd test_files && ../BINARY special '/\\[1\\]/' > /dev/null", "cd test_files && cat xx00", "cd test_files && cat xx01"], "cmd_post": "rm -rf test_files", "idx": 5}
{"name": "large_line_numbers", "description": "Test with large line numbers", "alias_name": "", "check_file": false, "cmd_prep": "mkdir -p test_files && seq 1 100 | sed 's/^/Line /' > test_files/large", "cmd_target": "cd test_files && ../BINARY large 50 75 > /dev/null && test -f xx00 && test -f xx01 && test -f xx02", "cmd_post": "rm -rf test_files", "idx": 6}
